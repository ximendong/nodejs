 <!-- 结束位置 -->
  <%
   
    // console.log(typeof page); 
      var p=parseInt(page)+2;
      if(p<5){
        p=5;
      }
  %>
  <%
  //情况是page=5
  for(var i=page-2;i<=p;i++){
        if(i<1||i>pageMax){
        continue
      }

  %>
   <li><a href="/?page=<%=i%>"><%=i%></a></li>
  <%}%>
<!-- pageMax-(parseInt(page)+2)>=1 证明后面还有-->
